<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/dial_panel_back"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal" >

        <LinearLayout
            android:id="@+id/dialicon"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" 
            android:background="@drawable/clickeffect">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:src="@drawable/dialicon" />

            <TextView
                android:layout_marginTop="10dp"
                android:id="@+id/dialertextview"
                android:ellipsize="start"
                android:layout_width="280dp"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:textSize="24sp"/>
        </LinearLayout>

        <ImageView
            android:id="@+id/delicon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/clickeffect"
            android:src="@drawable/delicon" />

    </LinearLayout>

    <!-- row rim -->

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal" >

       <TextView
            android:id="@+id/num1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/num1"
            android:gravity="center_vertical"
            android:tag="1"
            android:textColor="#B7B7B7"
            android:textSize="15sp" />

        <!-- col rim -->
>

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#000000"
            android:paddingRight="1dp" />

       <TextView
            android:id="@+id/num2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/num2"
            android:gravity="center_vertical"
            android:tag="2"
            android:text="ABC"
            android:textColor="#B7B7B7"
            android:textSize="15sp" />

        <!-- col rim -->
>

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#000000"
            android:paddingRight="1dp" />

        <TextView
            android:id="@+id/num3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/num3"
            android:gravity="center_vertical"
            android:tag="3"
            android:text="DEF"
            android:textColor="#B7B7B7"
            android:textSize="15sp" />
    </LinearLayout>

    <!-- row rim -->

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/num4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/num4"
            android:gravity="center_vertical"
            android:tag="4"
            android:text="GHI"
            android:textColor="#B7B7B7"
            android:textSize="15sp" />
        <!-- col rim -->
>

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#000000"
            android:paddingRight="1dp" />

        <TextView
            android:id="@+id/num5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/num5"
            android:gravity="center_vertical"
            android:tag="5"
            android:text="JKL"
            android:textColor="#B7B7B7"
            android:textSize="15sp" />
        <!-- col rim -->
>

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#000000"
            android:paddingRight="1dp" />

       <TextView
            android:id="@+id/num6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/num6"
            android:gravity="center_vertical"
            android:tag="6"
            android:text="MNO"
            android:textColor="#B7B7B7"
            android:textSize="15sp" />
    </LinearLayout>

    <!-- row rim -->

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/num7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/num7"
            android:gravity="center_vertical"
            android:tag="7"
            android:text="PQRS"
            android:textColor="#B7B7B7"
            android:textSize="15sp" />
        <!-- col rim -->
>

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#000000"
            android:paddingRight="1dp" />

        <TextView
            android:id="@+id/num8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/num8"
            android:gravity="center_vertical"
            android:tag="8"
            android:text="TUV"
            android:textColor="#B7B7B7"
            android:textSize="15sp" />
        <!-- col rim -->
>

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#000000"
            android:paddingRight="1dp" />

        <TextView
            android:id="@+id/num9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/num9"
            android:gravity="center_vertical"
            android:tag="9"
            android:text="WXYZ"
            android:textColor="#B7B7B7"
            android:textSize="15sp" />
    </LinearLayout>

    <!-- row rim -->

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#000000" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/numstar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="7dp"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/numstar"
            android:gravity="center_vertical"
            android:tag="*"/>
        <!-- col rim -->
>

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#000000"
            android:paddingRight="1dp" />

        <TextView
            android:id="@+id/num0"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/num0"
            android:gravity="center_vertical"
            android:tag="0"
            android:text="+"
            android:textColor="#B7B7B7"
            android:textSize="15sp" />
        <!-- col rim -->
>

        <TextView
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#000000"
            android:paddingRight="1dp" />

        <TextView
            android:id="@+id/poundkey"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="7dp"
            android:background="@drawable/clickeffect"
            android:drawableLeft="@drawable/poundkey"
            android:gravity="center_vertical"
            android:tag="#"/>
    </LinearLayout>

</LinearLayout>